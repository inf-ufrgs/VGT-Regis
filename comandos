docker import container-vgt.tar vgtregis

docker run --gpus all -it --name vgt --entrypoint /bin/bash -w /usr/src/app/VGT-Regis vgtregis
docker start vgt
docker exec -t vgt bash

apt-get update
apt-get install libleptonica-dev tesseract-ocr libtesseract-dev python3-pil tesseract-ocr-eng tesseract-ocr-script-latn
